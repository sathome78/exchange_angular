{
  "name": "exrates-front-new",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=dev --aot",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "to-csv-app": "ngx-translate-extract-csv -i ./translate/baseJSON -o translate/csv/app.csv -l _app",
    "to-csv-about-us": "ngx-translate-extract-csv -i ./translate/baseJSON -o translate/csv/about-us.csv -l about-us_base",
    "to-csv-privacy-policy": "ngx-translate-extract-csv -i ./translate/baseJSON -o translate/csv/privacy-policy.csv -l privacy-policy_base",
    "to-csv-contacts": "ngx-translate-extract-csv -i ./translate/baseJSON -o translate/csv/contacts.csv -l contacts_base",
    "to-csv-terms-and-conditions": "ngx-translate-extract-csv -i ./translate/baseJSON -o translate/csv/terms-and-conditions.csv -l terms-and-conditions_base",
    "to-extract-all-csv": " npm run to-csv-app && npm run to-csv-about-us && npm run to-csv-privacy-policy && npm run to-csv-contacts && npm run to-csv-terms-and-conditions",
    "extract-dashboard": "ngx-translate-extract --input ./src/app/settings ./src/app/orders ./src/app/funds ./src/app/dashboard ./src/app/referral ./src/app/popups ./src/app/header ./src/app/auth ./src/app/footer --output ./translate/baseJSON/_app.json --clean --sort --format json",
    "extract-privacy-policy": "ngx-translate-extract --input ./src/app/static-pages/privacy-policy --output ./translate/baseJSON/privacy-policy_base.json --clean --sort --format json",
    "extract-about-us": "ngx-translate-extract --input ./src/app/static-pages/about-us --output ./translate/baseJSON/about-us_base.json --clean --sort --format json",
    "extract-contacts": "ngx-translate-extract --input ./src/app/static-pages/contacts --output ./translate/baseJSON/contacts_base.json --clean --sort --format  json",
    "extract-terms-and-conditions": "ngx-translate-extract --input ./src/app/static-pages/terms-and-conditions --output ./translate/baseJSON/terms-and-conditions_base.json --clean --sort --format json",
    "extract-all": "npm run extract-privacy-policy && npm run extract-about-us && npm run extract-contacts && npm run extract-terms-and-conditions && npm run extract-dashboard"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^7.0.2",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@ng-select/ng-select": "^2.13.0",
    "@ngrx/effects": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "@ngrx/store-devtools": "^6.1.2",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@stomp/ng2-stompjs": "^6.1.0-beta.4",
    "angular2-moment": "^1.9.0",
    "angular2-qrcode": "^2.0.1",
    "angular2gridster": "^6.0.4",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "file-saver": "^2.0.0",
    "geetest": "^4.1.2",
    "jwt-decode": "^2.2.0",
    "mydatepicker": "^2.6.5",
    "ng-jwt": "^1.1.11",
    "ng-recaptcha": "^4.2.1",
    "ng5-slider": "^1.1.3",
    "ngx-infinite-scroll": "^6.0.1",
    "ngx-pagination": "^3.2.1",
    "ngx-perfect-scrollbar": "^7.0.0",
    "ngx-scrollbar": "^2.2.0",
    "ngx-webcam": "^0.2.0",
    "npm": "^6.7.0",
    "reselect": "^3.0.0",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.3.3",
    "sockjs-client": "^1.3.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.5",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^1.10.31",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "google-charts": "^1.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngx-translate-extract-csv": "^1.1.1",
    "protractor": "~5.4.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
